---
option/xml-prelude/show:        1;
option/doctype/show:            1;

output/filepath:                .;

output/program/xml:             | xmllint --format - > unicode.xml;
output/program/pdf:             | wkhtmltopdf - unicode.pdf;
---
$html [
  $body [
    $p [
      Unicode letters: \\xAE
    ]
  ]
]
