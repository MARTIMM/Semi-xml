html [
  head [
  
  ]

  body [

    $!td.test title='Integer manipulations' [

      $!td.doc [
        Test the integer declaration and then assignment of a number. Test
        if the number is assignable.
      ]

      $!td.prepair [!
        my Int $i;
      !]

      $!td.is result=$i expect=Any [Uninitialized Int]

      $!td.prepair [!
        $i = 10;
      !]

      $!td.is result=$i expect=10 [Initialized Int ]
    ]
  ]
]
